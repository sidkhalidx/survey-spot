<% @form_submission.form.fields.each do |field| %>
  <div>
    <%= field.title %>
  </div>
  <div class="p-2 border">
    <% field.answers.where(form_submission_id: @form_submission.id).each do |answer| %>
      <% if field.field_type=="FileField" %>
        <% answer.images.each do |image| %>
          <%= link_to image.filename, rails_blob_path(image, disposition: 'preview' ) %>  <%= image_tag(image, height: '25') %>
          <br/>
        <% end %>
      <% else %>
        <%= answer.answer %><br/>
      <% end %>
    <% end %>
  </div>
<% end %>