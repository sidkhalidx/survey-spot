<label><%= field.title %></label>
<%= '*required' if field.is_required %>
<br/>
<% temp=get_model_object(field, @form_submission.id) %>
<%= f.fields_for :answers, temp do |a| %>
  <%= a.hidden_field :field_id, value: field.id %>
  <%= a.date_field :answer, class:"form-control", value: temp.try(:answer).try(:to_date), max: get_date_constraint_from_fields(field, 'Maximum'), min: get_date_constraint_from_fields(field, 'Minimum') %>
<% end %>